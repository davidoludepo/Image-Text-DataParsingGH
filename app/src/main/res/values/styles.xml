<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="VendorActivity" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorPrimary">@color/colorSecondary</item>
        <item name="android:colorPrimaryDark">@color/colorSecondaryDark</item>
        <item name="android:colorAccent">@color/colorCcCent</item>
        <item name="android:navigationBarColor">@color/EachActivityNavigationBar</item>
        <item name="android:textColorSecondary">@color/EachToolbarIconColor</item>
    </style>

</resources>
